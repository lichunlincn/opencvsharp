{
  "version": "1.0.0-*",
  "dependencies": {
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    },
    "net20": {
      "dependencies": {
      },
      "frameworkAssemblies": {
        "System.Security": "2.0.0.0"
      },
      "buildOptions": {
        "define": [ "DOTNET_FRAMEWORK", "net20" ]
      }
    },
    "net40": {
      "dependencies": {
      },
      "frameworkAssemblies": {
        "System.Security": "4.0.0.0"
      },
      "buildOptions": {
        "define": [ "DOTNET_FRAMEWORK", "net40" ]
      }
    },
    "net46": {
      "dependencies": {
      },
      "frameworkAssemblies": {
        "System.Security": "4.0.0.0"
      },
      "buildOptions": {
        "define": [ "DOTNET_FRAMEWORK", "net46" ]
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "optimize": true,
    "keyFile": "Key.snk",
    "allowUnsafe": true,
    "copyToOutput": { "include": [ "OpenCvSharp.dll.config" ] }
  },
  "configurations": {
    "Release-JP": {
      "buildOptions": {
        "define": [ "LANG_JP" ]
      }
    }
  }
}
